<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="computerProperty" class="jun.spring.ch1.ioc.POJO.Computer">
        <property name="name" value="Property"/>
        <property name="printer" ref="printer"/>
    </bean>

    <bean id="computerConstructor" class="jun.spring.ch1.ioc.POJO.Computer">
        <constructor-arg index="0" value="Constructor"/>
        <constructor-arg index="1" ref="printer"/>
    </bean>

    <bean id="computerConstructorType" class="jun.spring.ch1.ioc.POJO.Computer">
        <constructor-arg type="java.lang.String" value="TypeConstructor"/>
        <constructor-arg type="jun.spring.ch1.ioc.POJO.Printer" ref="printer"/>
    </bean>

    <bean id="printer" class="jun.spring.ch1.ioc.POJO.ConsolePrinter"/>

</beans>